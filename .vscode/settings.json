{
	"editor.detectIndentation": false,
	"editor.insertSpaces": false,
	"editor.tabSize": 4,
	"editor.autoIndent": "full",
	"editor.trimAutoWhitespace": true,
	"files.trimTrailingWhitespace": true,
	"editor.formatOnSave": true,
	"C_Cpp.clang_format_style": "{ BasedOnStyle: Google, UseTab: ForIndentation, IndentWidth: 4, TabWidth: 4, IncludeBlocks: Preserve, ColumnLimit: 0, SortIncludes: false}",
	"search.exclude": {
		"dependencies": true,
		"submodules": true,
		"libs": true,
	},
	"telemetry.enableTelemetry": false,
	"cmake.buildDirectory": "${workspaceFolder}/build",
	"cmake.configureArgs": [
		"-DEQEMU_BUILD_LOGIN=ON",
		"-DEQEMU_BUILD_LUA=ON",
		"-DYAML_BUILD_SHARED_LIBS=ON",
		"-DYAML_CPP_BUILD_TOOLS=OFF",
		"Unix Makefiles"
	],
	"cmake.skipConfigureIfCachePresent": true,
	"testMate.cpp.test.executables": "{build,Build,BUILD,out,Out,OUT}/**/*{test,Test,TEST}*",
	"cmake.configureOnOpen": false,
	"files.associations": {
		"*.ipp": "cpp",
		"functional": "cpp",
		"string": "cpp",
		"iostream": "cpp",
		"map": "cpp",
		"fstream": "cpp",
		"type_traits": "cpp",
		"utility": "cpp",
